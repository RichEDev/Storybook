    <SqlCmdVariable Include="Path2">
      <Value>C:\Program Files\Microsoft SQL Server\MSSQL10.MSSQLSERVER\MSSQL\DATA\</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="targetExpenses">
      <Value>metabaseExpenses</Value>
    </SqlCmdVariable>
