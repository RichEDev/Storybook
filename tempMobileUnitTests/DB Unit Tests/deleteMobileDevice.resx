  <data name="testInitializeAction.SqlScript" xml:space="preserve">
    <value>declare @employeeId int;
declare @mobileDeviceID INT = 0;
declare @deviceTypeID INT = 4; -- iPhone (from metabase)
declare @deviceName NVARCHAR(100) = 'UnitTestDevice';
declare @pairingKey nvarchar(30) = '00999-88888-222222';
declare @serialKey nvarchar(200) = 'be21d28605cf201291c4a2ad0ae93a5f0d141aaa';
declare @requestorEmployeeId int;

begin try
	/* Create Employee Record */
	insert into employees 
	(username, password, title, firstname, surname, passwordMethod, cacheExpiry, CreationMethod)
	values
	('mobileDevice_unitTestUser', 'vlUnCTgO1bFJXxfib/suGQ==', 'Mr', 'mobileDevice', 'unitTester', 4, GETUTCDATE(), 0);

	set @employeeId = SCOPE_IDENTITY();

	if @employeeId &gt; 0
		select 1;
	else
		select 0;

	/* Create Employee Record */
	insert into employees 
	(username, password, title, firstname, surname, passwordMethod, cacheExpiry, CreationMethod)
	values
	('mobileDevice_unitTestRequestor', 'vlUnCTgO1bFJXxfib/suGQ==', 'Mr', 'mobileDevice', 'unitTesterRequestor', 4, GETUTCDATE(), 0);

	set @requestorEmployeeId = SCOPE_IDENTITY();

	if @requestorEmployeeId &gt; 0
		select 1;
	else
		select 0;

	exec @mobileDeviceID = dbo.saveMobileDevice
	@mobileDeviceID,
	@employeeId,
	@deviceTypeID,
	@deviceName,
	@pairingKey,
	@serialKey,
	@requestorEmployeeId;

	if @mobileDeviceID &gt; 0
		select 1;
	else
		select 0;

end try
begin catch
	select -999;
	select -999;
	select -999;
end catch</value>
  </data>
  <data name="dbo_deleteMobileDeviceSuccess_TestAction.SqlScript" xml:space="preserve">
    <value>declare @employeeId INT = (select employeeId from employees where username = 'mobileDevice_unitTestUser');
declare @mobileDeviceID INT = (select mobileDeviceId from mobileDevices where deviceName = 'UnitTestDevice' and employeeId = @employeeId);

begin try
	declare @rowCount int;

	exec @rowCount = dbo.deleteMobileDevice @mobileDeviceID, @employeeId, null;

	if @rowCount &gt; 0
		select 1;
	else
		select 0;

	select * from auditLog where employeeID = @employeeId and elementID = 165 and entityid = @mobileDeviceID and [action] = 3; -- delete
end try
begin catch
	select -999;
	select * from employees where employeeid = -1;
end catch</value>
  </data>
  <data name="testCleanupAction.SqlScript" xml:space="preserve">
    <value>declare @employeeId int = (select employeeId from employees where username = 'mobileDevice_unitTestUser');

delete from mobileDevices where employeeid = @employeeId;
delete from employees where username = 'mobileDevice_unitTestUser';
delete from employees where username = 'mobileDevice_unitTestRequestor';</value>
  </data>
